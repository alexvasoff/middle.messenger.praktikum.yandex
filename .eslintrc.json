{
  "extends": "airbnb",
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "rules": {
    "no-unused-vars": 2,
    "max-len": [1, 100],
    "import/prefer-default-export": "off",
    "arrow-parens": "off",
    "import/extensions": "off",
    "no-underscore-dangle": "off"
  }
}
